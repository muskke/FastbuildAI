{
    "name": "@buildingai/wechat-sdk",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "require": "./dist/*.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist .turbo tsconfig.tsbuildinfo",
        "check-types": "tsc --noEmit",
        "build": "rimraf dist && tsc -p tsconfig.build.json",
        "dev": "tsc -p tsconfig.build.json --watch",
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -p tsconfig.build.json --watch",
        "test": "jest",
        "test:dev": "jest --watch"
    },
    "dependencies": {
        "qrcode": "^1.5.4",
        "wechat-crypto": "^0.0.2",
        "wechatpay-node-v3": "^2.2.1",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {
        "@buildingai/constants": "workspace:*",
        "@buildingai/eslint-config": "workspace:*",
        "@buildingai/typescript-config": "workspace:*",
        "@buildingai/utils": "workspace:*",
        "@types/qrcode": "^1.5.5",
        "@types/xml2js": "^0.4.14"
    }
}