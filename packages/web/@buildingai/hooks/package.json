{
    "name": "@buildingai/hooks",
    "version": "0.0.0",
    "private": true,
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*.ts"
    },
    "scripts": {
        "check-types": "vue-tsc --noEmit",
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix"
    },
    "sideEffects": false,
    "devDependencies": {
        "@buildingai/eslint-config": "workspace:*",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.7.0"
    },
    "dependencies": {
        "@nuxt/ui": "catalog:web",
        "@vueuse/core": "catalog:web",
        "vue": "catalog:web",
        "@fancyapps/ui": "^6.0.32",
        "@buildingai/ui": "workspace:*"
    },
    "peerDependencies": {
        "@buildingai/service": "workspace:*",
        "@buildingai/http": "workspace:*",
        "@buildingai/stores": "workspace:*"
    }
}