{
    "name": "@buildingai/i18n-config",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "description": "通用的 Nuxt i18n 配置加载器，支持动态路径和插件系统",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./src/*": {
            "types": "./src/*.ts",
            "import": "./src/*.ts"
        }
    },
    "scripts": {
        "check-types": "vue-tsc --noEmit",
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -p tsconfig.build.json --watch"
    },
    "dependencies": {},
    "devDependencies": {
        "@buildingai/eslint-config": "workspace:*",
        "@buildingai/typescript-config": "workspace:*"
    },
    "peerDependencies": {
        "nuxt": "^3.0.0"
    }
}