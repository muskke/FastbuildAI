{
    "name": "@buildingai/nuxt",
    "version": "0.0.0",
    "type": "module",
    "exports": {
        ".": "./src/index.ts",
        "./nuxt.config": "./src/nuxt.config.ts",
        "./router": "./src/router.ts",
        "./middleware": "./src/middleware.ts",
        "./modules/*": "./src/modules/*",
        "./plugins/*": "./src/plugins/*",
        "./styles/*": "./src/assets/styles/*",
        "./stores/*": "./src/stores/*"
    },
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "files": [
        "src"
    ],
    "scripts": {
        "check-types": "vue-tsc --noEmit",
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -p tsconfig.build.json --watch"
    },
    "dependencies": {
        "@buildingai/config": "workspace:*",
        "@buildingai/layouts": "workspace:*",
        "@buildingai/stores": "workspace:*",
        "@buildingai/ui": "workspace:*",
        "@buildingai/service": "workspace:*",
        "@nuxt/kit": "catalog:web",
        "defu": "^6.1.4"
    },
    "devDependencies": {
        "@buildingai/eslint-config": "workspace:*",
        "@buildingai/i18n-config": "workspace:*"
    },
    "peerDependencies": {
        "@buildingai/constants": "workspace:*",
        "@buildingai/hooks": "workspace:*"
    }
}