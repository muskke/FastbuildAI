{
    "name": "@buildingai/service",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "check-types": "vue-tsc --noEmit",
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -p tsconfig.build.json --watch"
    },
    "type": "module",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "main": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./common": "./src/common.ts",
        "./webapi/*": "./src/webapi/*.ts",
        "./consoleapi/*": "./src/consoleapi/*.ts",
        "./models/*": "./src/models/*.d.ts"
    },
    "files": [
        "src"
    ],
    "dependencies": {
        "@buildingai/constants": "workspace:*"
    },
    "devDependencies": {
        "@buildingai/types": "workspace:*",
        "@buildingai/eslint-config": "workspace:*",
        "@buildingai/typescript-config": "workspace:*",
        "@tsconfig/node22": "catalog:dev"
    }
}