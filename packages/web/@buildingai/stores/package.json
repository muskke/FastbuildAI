{
    "name": "@buildingai/stores",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "check-types": "vue-tsc --noEmit"
    },
    "type": "module",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        "./*": {
            "types": "./src/*.ts",
            "import": "./src/*.ts"
        },
        "./src/*": {
            "types": "./src/*.ts",
            "import": "./src/*.ts"
        }
    },
    "files": [
        "src"
    ],
    "devDependencies": {
        "@buildingai/eslint-config": "workspace:*",
        "@buildingai/constants": "workspace:*",
        "@buildingai/typescript-config": "workspace:*",
        "@tsconfig/node22": "catalog:dev"
    },
    "peerDependencies": {
        "@buildingai/service": "workspace:*"
    }
}