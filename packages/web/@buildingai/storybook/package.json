{
    "name": "@buildingai/storybook",
    "version": "0.0.0",
    "type": "module",
    "description": "BuildingAI Storybook configuration and utilities",
    "main": "./index.ts",
    "exports": {
        ".": {
            "types": "./index.ts",
            "import": "./index.ts"
        },
        "./main": {
            "types": "./main.ts",
            "import": "./main.ts"
        },
        "./preview": {
            "types": "./preview.ts",
            "import": "./preview.ts"
        }
    },
    "files": [
        "*.ts"
    ],
    "keywords": [
        "storybook",
        "vue",
        "nuxt",
        "buildingai",
        "configuration"
    ],
    "peerDependencies": {
        "@storybook-vue/nuxt": "^9.0.0",
        "@chromatic-com/storybook": "^4.0.0",
        "@storybook/addon-docs": "^9.0.0",
        "storybook": "^9.0.0"
    },
    "devDependencies": {
        "@buildingai/eslint-config": "workspace:*"
    },
    "scripts": {
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -p tsconfig.build.json --watch"
    }
}