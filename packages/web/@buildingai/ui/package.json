        "@tiptap/extension-image": "catalog:web",
        "@tiptap/extension-link": "catalog:web",
        "@tiptap/extension-list-item": "catalog:web",
        "@tiptap/extension-placeholder": "catalog:web",
        "@tiptap/extension-text-style": "catalog:web",
        "@tiptap/extension-underline": "catalog:web",
        "@tiptap/starter-kit": "catalog:web",
        "@tiptap/vue-3": "catalog:web",
        "@vueuse/core": "catalog:web",
        "echarts": "catalog:web",
