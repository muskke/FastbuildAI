{
    "name": "@buildingai/ui",
    "version": "0.0.0",
    "private": true,
    "types": "./env.d.ts",
    "exports": {
        "./*": "./src/*",
        "./bd-modal-use": {
            "types": "./src/components/bd-modal-use/index.d.ts",
            "import": "./src/components/bd-modal-use/index.vue",
            "require": "./src/components/bd-modal-use/index.vue"
        },
